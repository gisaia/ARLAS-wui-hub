{
  "name": "arlas-hub",
  "version": "25.1.0",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "export NODE_OPTIONS=--max_old_space_size=8192 && ng build --configuration production --aot --base-href='$ARLAS_HUB_BASE_HREF/'",
    "test": "ng test",
    "lint": "ng lint",
    "postinstall": "patch-package --patch-dir ./node_modules/arlas-web-core/patches && patch-package --patch-dir ./node_modules/arlas-d3/patches",
    "e2e": "ng e2e",
    "license-check": "license-check-and-add check -f license-check-and-add.json",
    "license-add": "license-check-and-add add -f license-check-and-add.json",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,fr}.json --format json"
  },
  "private": true,
  "peerDependencies": {
    "@angular/animations": "~18.2.13",
    "@angular/cdk": "~17.3.10",
    "@angular/common": "~18.2.13",
    "@angular/compiler": "~18.2.13",
    "@angular/core": "~18.2.13",
    "@angular/forms": "~18.2.13",
    "@angular/material": "~17.3.10",
    "@angular/platform-browser": "~18.2.13",
    "@angular/platform-browser-dynamic": "~18.2.13",
    "@angular/router": "~18.2.13",
    "rxjs": "~7.4.0",
    "zone.js": "^0.14.10"
  },
  "dependencies": {
    "@ngx-translate/core": "14.0.0",
    "arlas-wui-toolkit": "27.0.0-beta.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular-eslint/builder": "^18.2.12",
    "@angular-eslint/eslint-plugin": "^18.2.12",
    "@angular-eslint/eslint-plugin-template": "^18.2.12",
    "@angular-eslint/schematics": "^18.2.12",
    "@angular-eslint/template-parser": "^18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.13",
    "@angular/language-service": "^18.2.13",
    "@biesbjerg/ngx-translate-extract-marker": "1.0.0",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^18.19.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vendure/ngx-translate-extract": "8.3.0",
    "eslint": "^8.28.0",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "license-check-and-add": "^4.0.5",
    "node": "^18.19.0",
    "patch-package": "^6.4.7",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "typescript": "5.4.5"
  },
  "overrides": {
    "angular-oauth2-oidc-jwks": {
      "jsrsasign": "^11.0.0"
    },
    "braces": "3.0.3",
    "socks": "2.7.3",
    "@angular-devkit/build-angular": {
      "webpack-dev-middleware": "~5.3.4"
    },
    "http-proxy-middleware": "2.0.7",
    "shpjs": "4.0.4",
    "@mapbox/geojson-extent": {
      "traverse": "0.6.8"
    },
    "dompurify": "2.5.6",
    "mermaid": "10.9.3"
  }
}